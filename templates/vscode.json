    "editorGutter.commentUnresolvedGlyphForeground": "$yellow",
    "list.errorForeground": "$red",
    "list.warningForeground": "$yellow"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "$fg"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "$fg"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "$comment"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "$string"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "$keyword"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "$fg"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "$constant"
      }
    },
    {
      "name": "Property",
      "scope": "variable.other.property",
      "settings": {
        "foreground": "$property"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "$constant"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "$constant"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "$fg"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "$keyword"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "$keyword"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "fontStyle": "",
        "foreground": "$type"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.type, entity.name.class, entity.name.namespace, entity.name.scope-resolution",
      "settings": {
        "fontStyle": "",
        "foreground": "$function"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "",
        "foreground": "$function"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "$function"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "",
        "foreground": "$parameter"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "$keyword"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "$function"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "$type"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "$type"
      }
    },
    {
      "name": "Library class/type",
      "scope": "support.type, support.class",
      "settings": {
        "fontStyle": "",
        "foreground": "$type"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "",
        "foreground": "$red"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "$red"
      }
    },
    {
      "name": "JSON String",
      "scope": "meta.structure.dictionary.json string.quoted.double.json",
      "settings": {
        "foreground": "$fg"
      }
    },
    {
      "name": "diff.header",
      "scope": "meta.diff, meta.diff.header",
      "settings": {
        "foreground": "$base_bg"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "$keyword"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "$function"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "$string"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "$propertyA0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "$string"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "$keyword"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "$string"
      }
    },
    {
      "name": "Markup Styling",
      "scope": "markup.bold, markup.italic",
      "settings": {
        "foreground": "$type"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "$parameter"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "foreground": "$function"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "$function",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": "markup.quote.markdown",
      "settings": {
        "fontStyle": "italic",
        "foreground": "$base_bg"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Link Title/Description",
      "scope": "string.other.link.title.markdown,string.other.link.description.markdown",
      "settings": {
        "foreground": "$property"
      }
    },
    {
      "name": "Markdown Underline Link/Image",
      "scope": "markup.underline.link.markdown,markup.underline.link.image.markdown",
      "settings": {
        "foreground": "$string"
      }
    },
    {
      "name": "Markdown Emphasis",
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markdown Punctuation Definition Link",
      "scope": "markup.list.unnumbered.markdown, markup.list.numbered.markdown",
      "settings": {
        "foreground": "$fg"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "$function"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "$blue"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "$yellow"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "$red"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "$blue"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "$parameter"
      }
    },
    {
      "name": "Punctuation",
      "scope": "punctuation.separator",
      "settings": {
        "foreground": "$fg"
      }
    },
    {
      "name": "Punctuation",
      "scope": "support.constant.property-value.css",
      "settings": {
        "foreground": "$function"
      }
    }
  ],
  "semanticHighlighting": true
}
