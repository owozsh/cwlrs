    "statusBarItem.remoteBackground": "$keyword",
    "statusBar.noFolderForeground": "$base_darkest",
    "statusBar.debuggingForeground": "$base_darkest",
    "statusBarItem.remoteForeground": "$base_darkest",
    "statusBarItem.hoverForeground": "$base_darkest",
    "ports.iconRunningProcessForeground": "$fg",
    "activityBarBadge.background": "$keyword",
