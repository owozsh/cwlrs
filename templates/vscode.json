    "settings.focusedRowBackground": "$base_bg5A",
    "titleBar.border": "$border",
    "titleBar.activeBackground": "$base_darker",
    "titleBar.inactiveBackground": "$base_dark",
    "statusBar.background": "$base_darker",
    "statusBar.foreground": "$fg",
    "statusBar.noFolderBackground": "$blue10",
