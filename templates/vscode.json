    "pickerGroup.foreground": "$base_bg",
    "input.background": "$base_bg",
    "inputOption.activeBorder": "$base_bg",
    "focusBorder": "$accent30",
    "editorWidget.background": "$base_bg",
    "debugToolBar.background": "$base_dark",
    "diffEditor.insertedTextBackground": "$green80",
    "diffEditor.removedTextBackground": "$red70",
    "inputValidation.errorBackground": "$red4A",
    "inputValidation.errorBorder": "$red",
    "inputValidation.warningBackground": "$yellow",
    "inputValidation.warningBorder": "$yellow",
    "inputValidation.infoBackground": "$blue",
    "inputValidation.infoBorder": "$blue",
    "editorHoverWidget.background": "$base_bg",
    "editorHoverWidget.border": "$base_bg",
    "editorSuggestWidget.background": "$base_dark",
    "editorSuggestWidget.border": "$base_bg",
    "editorGroup.border": "$base_bg",
    "peekView.border": "$base_bg",
    "peekViewEditor.background": "$base_dark",
    "peekViewResult.background": "$base_dark",
    "peekViewTitle.background": "$base_dark",
    "peekViewResult.selectionBackground": "$accent30",
    "peekViewResult.matchHighlightBackground": "$base_bg",
    "peekViewEditor.matchHighlightBackground": "$base_bg",
    "terminal.ansiBlack": "$base_darkest",
    "terminal.ansiRed": "$red",
