    "gitDecoration.stageModifiedResourceForeground": "$cyan",
    "gitDecoration.stageDeletedResourceForeground": "$magenta",
    "gitDecoration.untrackedResourceForeground": "$cyan",
    "gitDecoration.ignoredResourceForeground": "$magenta",
    "gitDecoration.conflictingResourceForeground": "$red",
    "gitDecoration.submoduleResourceForeground": "$blue",
    "editorGutter.modifiedBackground": "$blue",
    "editorGutter.addedBackground": "$green",
    "editorGutter.deletedBackground": "$red",
    "editorGutter.commentRangeForeground": "$cyan",
