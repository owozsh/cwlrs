    "terminal.ansiBlue": "$blue",
    "terminal.ansiMagenta": "$magenta",
    "terminal.ansiCyan": "$cyan",
    "terminal.ansiWhite": "$base_lightest",
    "terminal.ansiBrightBlack": "$base_darkest",
    "terminal.ansiBrightRed": "$red",
